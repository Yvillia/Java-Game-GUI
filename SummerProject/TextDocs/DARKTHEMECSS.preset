.Button
{
	-fx-background-color:#cacae0;
	-fx-text-color: #ffffff;
}

.Button:hover
{
	-fx-background-color: #a5b3c9;
	-fx-text-color: #ffffff;
}

.root
{
	-fx-background-color: #000000;
	-fx-border-color: #f90230;
}

.BackButton
{
	-fx-padding: 10 10 10 10;
        -fx-position: absolute; 
        -fx-vertical-align: middle;
}
                     
.BackButton:hover
{
        -fx-padding: 10 10 10 10; 
        -fx-letter-spacing: 3;
        -fx-vertical-align: middle;
        -fx-position: absolute;
	-fx-background-color: #bab1b1;
}

.Text
{
	-fx-font-family: serif;
	-fx-text-color: #000000;
	-fx-background-color: #ffffff;
}